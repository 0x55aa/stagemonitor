{
	"title": "EhCache",
	"services": {
		"filter": {
			"list": [
				{
					"type": "filter",
					"name": "application",
					"query": "stagemonitor.*",
					"includeAll": false
				},
				{
					"type": "filter",
					"name": "instance",
					"query": "stagemonitor.[[application]].*",
					"includeAll": true
				},
				{
					"type": "filter",
					"name": "host",
					"query": "stagemonitor.[[application]].[[instance]].*",
					"includeAll": true
				},
				{
					"type": "filter",
					"name": "aggregation",
					"query": "",
					"includeAll": false,
					"options": [
						{
							"value": "averageSeries",
							"text": "avg"
						},
						{
							"value": "maxSeries",
							"text": "max"
						}
					],
					"current": {
						"value": "maxSeries",
						"text": "max"
					}
				}
			],
			"time": {
				"from": "now-6h",
				"to": "now"
			}
		}
	},
	"rows": [
		{
			"title": "",
			"height": "350px",
			"editable": true,
			"collapse": false,
			"collapsable": false,
			"panels": [
				{
					"span": 6,
					"editable": true,
					"type": "graph",
					"loadingEditor": false,
					"x-axis": true,
					"y-axis": true,
					"scale": 1,
					"grid": {
						"threshold1": 60,
						"threshold2": 40,
						"threshold1Color": "rgba(216, 200, 27, 0.27)",
						"threshold2Color": "rgba(234, 112, 112, 0.22)",
						"leftMin": 0,
						"leftMax": 100,
						"rightMin": null
					},
					"annotate": {
						"enable": false,
						"query": "*",
						"size": 20,
						"field": "_type",
						"sort": [
							"_score",
							"desc"
						]
					},
					"auto_int": true,
					"resolution": 100,
					"interval": "10m",
					"intervals": [
						"auto",
						"1s",
						"1m",
						"5m",
						"10m",
						"30m",
						"1h",
						"3h",
						"12h",
						"1d",
						"1w",
						"1y"
					],
					"lines": true,
					"fill": 0,
					"linewidth": 2,
					"points": false,
					"pointradius": 5,
					"bars": false,
					"stack": false,
					"spyable": true,
					"zoomlinks": false,
					"options": false,
					"legend": {
						"show": true,
						"values": true,
						"min": false,
						"max": false,
						"current": true,
						"total": false,
						"avg": true
					},
					"interactive": true,
					"legend_counts": true,
					"timezone": "browser",
					"percentage": false,
					"zerofill": true,
					"nullPointMode": "connected",
					"steppedLine": false,
					"tooltip": {
						"value_type": "cumulative",
						"query_as_alias": true
					},
					"targets": [
						{
							"target": "groupByNode(summarize(scale(lowestAverage(stagemonitor.[[application]].[[instance]].[[host]].cache.*.access.hit.total.ratio,10),100),'10minute','avg'),5,'[[aggregation]]')"
						}
