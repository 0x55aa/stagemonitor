{
	"groupBy": true,
	"data": [
		{
			"title": "Server Thread Pool usage",
			"target": "groupByNode(jawap.${application}.${instance}.${host}.server.threadpool.*.usage, ${group}, 'maxSeries')",
			"params": "yMax=1"

		}, {
			"title": "Number of Sessions",
			"target": [
				"groupByNode(jawap.${application}.${instance}.${host}.web.sessions.count,${group},'sumSeries')"
			],
			"params": "areaMode=stacked"
		}
	]
}