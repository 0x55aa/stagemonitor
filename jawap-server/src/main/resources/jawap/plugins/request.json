{
	"type": "request",
	"data": [
		{
			"title": "Response Time (ms)",
			"target": [
				"dashed(averageSeries(jawap.${application}.${environment}.${host}.request.${requestName}.p95))",
				"averageSeries(jawap.${application}.${environment}.${host}.request.${requestName}.mean)",
				"dashed(averageSeries(jawap.${application}.${environment}.${host}.request.${requestName}.min))"
			]
		},
		{
			"title": "Throughput (Requests per Minute)",
			"target": [
				"alias(sum(jawap.${application}.${environment}.${host}.request.${requestName}.m1_rate), 'total')",
			 	"alias(sum(jawap.${application}.${environment}.${host}.request.${requestName}.error.m1_rate), 'failed')"
			]
		}
	]

}