{
	"template": "stagemonitor-*",
	"settings" : {
		"index" : {
			"store" : { "compress" : { "stored" : true, "tv": true } },
			"refresh_interval" : "5s"
		}
	},
	"mappings": {
		"_default_": {
			"_ttl" : { "enabled" : true },
			"dynamic_templates" : [ {
				"http_headers": {
					"path_match": "headers.*",
					"mapping": { "type": "string", "index": "not_analyzed" }
				}
			},
			{
				"string_fields" : {
					"match" : "*",
					"match_mapping_type" : "string",
					"mapping" : {
						"type" : "string", "index" : "analyzed", "omit_norms" : true,
						"fields" : {
							"raw" : {"type": "string", "index" : "not_analyzed", "ignore_above" : 256}
						}
					}
				}
			} ],
			"_source" : {
				"excludes" : ["callStackJson"]
			},
			"properties": {
				"@timestamp": {
					"type": "date",
					"format": "dateOptionalTime"
				},
				"callStack": {
					"type": "string",
					"index": "no"
				},
				"callStackJson": {
					"type": "string",
					"index": "no",
					"store": "yes"
				},
				"clientIp": {
					"type": "ip"
				}
			}
		}
	}
}